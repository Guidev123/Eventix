{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": ""
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Eventix.Api"
    }
  },
  "KeyCloakExtensions": {
    "AuthorizationUrl": "",
    "TokenUrl": ""
  },
  "Authentication": {
    "Audience": "",
    "TokenValidationParameters": {
      "ValidIssuers": [ "", "" ]
    },
    "MetadataAddress": "",
    "RequireHttpsMetadata": true
  },
  "OTEL_EXPORTER_OTLP_ENDPOINT": "",
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
    },
    "Clusters": {
    }
  }
}